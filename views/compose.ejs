<%- include("partials/header") %>
<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input type="text" name="postTitle" class="form-control" autocomplete="off">
  </div>
  <div class="form-group">
    <label>Post</label>
    <textarea class="form-control" name="postBody" rows="8"></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>

<%- include("partials/footer") %>
